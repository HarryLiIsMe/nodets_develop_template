{
	"name": "nodets_develop_template",
	"version": "1.0.0",
	"scripts": {
		"fmt": "npx prettier --config .prettierrc.json --check 'src/*.ts' 'src/**/*.ts' 'tests/*.ts' 'tests/**/*.ts' '.eslintrc.js'",
		"fmt:fix": "npx prettier --config .prettierrc.json --write 'src/*.ts' 'src/**/*.ts' 'tests/*.ts' 'tests/**/*.ts' '.eslintrc.js'",
		"lint": "npx eslint -c .eslintrc.js",
		"lint:fix": "npx eslint -c .eslintrc.js --fix",
		"packs": "echo packs",
		"build": "webpack",
		"obfus": "javascript-obfuscator",
		"build_bin": "nexe",
		"clean": "rimraf",
		"test": "npx mocha --config .mocharc.json 'tests/**/*.ts'",
		"main1": "DEBUG=debug,error node -r ts-node/register -r tsconfig-paths/register -r dotenv/config src/main.ts",
		"main2": "DEBUG=debug,error ts-node -r tsconfig-paths/register -r dotenv/config src/main.ts"
	},
	"license": "MIT",
	"dependencies": {
		"debug": "^4.4.0",
		"dotenv": "^16.4.7",
		"sleep-promise": "^9.1.0",
		"util": "^0.12.5"
	},
	"devDependencies": {
		"@projectsophon/eslint-plugin-typescript-enum": "^2.2.0",
		"@rollup/plugin-node-resolve": "^16.0.1",
		"@rollup/plugin-typescript": "^12.1.2",
		"@types/chai": "^4.3.20",
		"@types/debug": "^4.1.12",
		"@types/mocha": "^10.0.10",
		"@types/node": "^20.17.10",
		"@typescript-eslint/eslint-plugin": "^8.18.2",
		"@typescript-eslint/eslint-plugin-tslint": "^7.0.2",
		"@typescript-eslint/parser": "^8.18.2",
		"@typescript-eslint/typescript-estree": "^8.18.2",
		"chai": "^4.5.0",
		"cpr": "^3.0.1",
		"cross-env": "^7.0.3",
		"cross-var": "^1.1.0",
		"eslint": "^9.17.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-import-resolver-typescript": "^3.7.0",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-prettier": "^5.2.1",
		"gulp-javascript-obfuscator": "^1.1.6",
		"javascript-obfuscator": "^4.1.1",
		"make-dir-cli": "^3.1.0",
		"mocha": "^10.8.2",
		"nexe": "5.0.0-beta.4",
		"nodemon": "^3.1.9",
		"npm-run-all": "^4.1.5",
